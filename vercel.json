{
  "version": 2,
  "builds": [
    {
      "src": "dist/regex-generator-app/server.mjs",
      "use": "@vercel/node"
    },
    {
      "src": "dist/regex-generator-app/browser/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(sitemap\\.xml|robots\\.txt)",
      "dest": "/dist/regex-generator-app/browser/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/dist/regex-generator-app/server.mjs"
    }
  ]
}
