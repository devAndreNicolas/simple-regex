{
  "rewrites": [
    {
      "source": "/sitemap.xml",
      "destination": "/sitemap.xml"
    },
    {
      "source": "/robots.txt",
      "destination": "/robots.txt"
    },
    {
      "source": "/(.*)",
      "destination": "/api/index"
    }
  ],
  "builds": [
    {
      "src": "dist/server/server.mjs",
      "use": "@vercel/node"
    }
  ],
  "outputDirectory": "dist/browser"
}