{
  "version": 2,
  "builds": [
    {
      "src": "dist/regex-generator-app/server/main.js",
      "use": "@vercel/node",
      "config": {
        "includeFiles": "dist/regex-generator-app/browser/**"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*\\.(?:css|js|map|xml|png|jpg|ico|gif|svg|woff2|woff|ttf))",
      "dest": "dist/regex-generator-app/browser/$1"
    },
    {
      "src": "/(.*)",
      "dest": "dist/regex-generator-app/server/main.js"
    }
  ]
}